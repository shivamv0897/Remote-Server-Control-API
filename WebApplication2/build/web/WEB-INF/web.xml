<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServerDetails</servlet-name>
        <servlet-class>ServerDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Details</servlet-name>
        <servlet-class>Details</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Universal</servlet-name>
        <servlet-class>Universal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StopProcess</servlet-name>
        <servlet-class>StopProcess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServiceDetails</servlet-name>
        <servlet-class>ServiceDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadDetails</servlet-name>
        <servlet-class>LoadDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Load</servlet-name>
        <servlet-class>Load</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StopService</servlet-name>
        <servlet-class>StopService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServerDetails</servlet-name>
        <url-pattern>/ServerDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Details</servlet-name>
        <url-pattern>/Details</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Universal</servlet-name>
        <url-pattern>/Universal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StopProcess</servlet-name>
        <url-pattern>/StopProcess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServiceDetails</servlet-name>
        <url-pattern>/ServiceDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadDetails</servlet-name>
        <url-pattern>/LoadDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Load</servlet-name>
        <url-pattern>/Load</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StopService</servlet-name>
        <url-pattern>/StopService</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
